
public class Produit {

    private String nom;
    private float prix;

    public String getNom() {
	return nom;
    }

    public void setNom(String nom) {
	this.nom = nom;
    }

    public float getPrix() {
	return prix;
    }

    public void setPrix(float prix) {
	this.prix = prix;
    }

}
