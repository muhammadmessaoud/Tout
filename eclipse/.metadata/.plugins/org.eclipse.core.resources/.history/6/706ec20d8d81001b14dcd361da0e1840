package Exo_constructeur;

public class Ordinateur {
	private String string;
	private String string2;
	private int i;
	private float f;
	
	// Créer propriété privée marque
    // Créer propriété privée modele
    // Créer propriété privée nbPoucesEcran
    // Créer propriété privée prix

    // Créer constructeur

    // Créer getter marque
    // Créer setter marque

    // Créer getter modele
    // Créer setter modele

    // Créer getter nbPoucesEcran
    // Créer setter nbPoucesEcran

    // Créer getter prix
    // Créer setter prix

    // Créer toString
	
	public Ordinateur(String marque, String model, int taille, float prix) {
		// TODO Auto-generated constructor stub
		string = marque;
		string2 = model;
		i = taille;
		f = prix;
	}
	

	public String getString() {
		return string;
	}


	public void setString(String string) {
		this.string = string;
	}


	public String getString2() {
		return string2;
	}


	public void setString2(String string2) {
		this.string2 = string2;
	}


	public int getI() {
		return i;
	}


	public void setI(int i) {
		this.i = i;
	}


	public float getF() {
		return f;
	}


	public void setF(float f) {
		this.f = f;
	}



	public String toString() {
		return "Marque : " + this.getString() + "\r" +
				"Model : " + this.getString2() + "\r" + 
				"Taille : " + this.getI() + "\r" + 
				"Prix : " + this.getF() + "\r" ;
	}
	
    

}
